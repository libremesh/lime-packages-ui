{
    "unauthenticated": {
        "description": "Pitbull voucher public access",
        "write": {
            "ubus": {
                    "lime-voucher": [ "auth_voucher" ]
            }
        }
    },
    "root": {
        "description": "Pitubll administration access",
        "write": {
            "ubus": {
                    "lime-voucher": ["*"]
            }
        },
        "read": {
            "ubus": {
                    "lime-voucher": ["*"]
            }
        }
    }
}